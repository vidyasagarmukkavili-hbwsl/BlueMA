    <body>

        <referenceContainer name="header-wrapper">
            
            <container name="text-and-search" htmlTag="div" htmlId="text-and-search-id">
              <block class="Magento\Framework\View\Element\Template" name="my.icons.store" template="Magento_Search::my_icons.phtml" before="top.search"/>
            </container>

        </referenceContainer>

             <move element="top.search" destination="text-and-search" after="my.icons.store" />

 </body>



</page>


