<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">

    <body>

    <referenceBlock name ="logo">
        <arguments>
                  <argument name="logo_file" xsi:type="string">images/logo.png</argument>
                    <argument name="logo_width" xsi:type="number">150</argument>
                    <argument name="logo_height" xsi:type="number">150</argument>
                    <argument name="logo_alt" xsi:type="string">FrontendLogo</argument>
        </arguments>
    </referenceBlock>

    </body>

    <referenceContainer name="footer-bottom-content">
        <block class="Magento\Framework\View\Element\Template" name="report.bugs.bottom" template="Magento_Theme::html/bugreport.phtml"/>

    </referenceContainer>


</page>
